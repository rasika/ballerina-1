<!DOCTYPE html>
<html>

{{> head }}

<body class="cBallerina-io">
    {{> bioBody }}
    <div class="navi-wrapper">
        <div class="navi-wrapper-content">
            {{> logo }}
            <div class="primary-list">
                <h3>Functions</h3>
                <ul>
                    {{#each module.functions}}
                        <li {{#equals functions.name name}}class="active"{{/equals}}>
                            <a href="#{{name}}">{{name}}</a>
                        </li>
                    {{/each}}
                </ul>
            </div>
            <div class="ui divider"></div>
            {{> currentModule }}
            {{> allModules }}
        </div>
    </div>

    <div class="ui vertical sidebar menu">
        <div class="navi-wrapper-sidebar">
            <div class="navi-wrapper-content">
                {{> logo }}
                <div class="primary-list">
                    <h3>Functions</h3>
                    <ul>
                        {{#each module.functions}}
                            <li {{#equals functions.name name}}class="active"{{/equals}}>
                                <a href="#{{name}}">{{name}}</a>
                            </li>
                        {{/each}}
                    </ul>
                </div>
                <div class="ui divider"></div>
                {{> currentModule }}
                {{> allModules }}
            </div>
        </div>
    </div>

    <div class="content-wrapper pusher">
        {{> mobMenu }}
        <div class="main-wrapper">
            <h1>
                {{> breadcrumb }}
                <span class="title-type">Functions - </span>
                {{#if project.isSingleFile}}
                    {{project.sourceFileName}}
                {{else}}
                    <a href="{{ rootPath }}{{module.id}}/index.html">{{module.id}}</a>
                {{/if}}
            </h1>
            <div class="constants">
                <table class="ui table details-table">
                    {{#each functions}}
                        <tbody>
                            <tr>
                                <td width="30%"><a class="functions {{#if isDeprecated}}strike{{/if}}" href="#{{name}}">{{name}}</a></td>
                                <td width="70%">
                                    <div class="function-desc">
                                        {{#if isDeprecated}}
                                            <span class="stab">Deprecated</span>
                                        {{/if}}
                                        {{{ description }}}
                                    </div>
                                </td>
                            </tr>
                        </tbody>
                    {{/each}}
                </table>
                <div>
                    {{#if functions}}
                        <div>
                            {{#each functions}}
                                {{> method }}
                            {{/each}}
                        </div>
                    {{/if}}
                </div>
            </div>
        </div>
    </div>
</body>

{{> footer }}
