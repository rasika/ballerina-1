<!DOCTYPE html>
<html>

{{> head }}

<body class="cBallerina-io">
    {{> bioBody }}
    <div class="navi-wrapper">
        <div class="navi-wrapper-content">
            {{> logo }}
            <div class="primary-list">
                <h3>Objects</h3>
                <ul>
                    {{#each module.objects}}
                        <li {{#equals object.name name}}class="active"{{/equals}}>
                            <a href="{{name}}.html">{{name}}</a>
                        </li>
                    {{/each}}
                </ul>
            </div>
            <div class="ui divider"></div>
            {{> currentModule }}
            {{> allModules }}
        </div>
    </div>

    <div class="ui vertical sidebar menu">
        <div class="navi-wrapper-sidebar">
            <div class="navi-wrapper-content">
                {{> logo }}
                <div class="primary-list">
                    <h3>Objects</h3>
                    <ul>
                        {{#each module.objects}}
                            <li {{#equals object.name name}}class="active"{{/equals}}></li>>
                                <a href="{{name}}.html">{{name}}</a>
                            </li>
                        {{/each}}
                    </ul>
                </div>
                <div class="ui divider"></div>
                {{> currentModule }}
                {{> allModules }}
            </div>
        </div>
    </div>
    
    <div class="content-wrapper pusher">
        {{> mobMenu }}

        <div class="main-wrapper">
            <h1>
                {{> breadcrumb }}
                <span class="title-type">Object - </span>
                {{#if project.isSingleFile}}
                    {{project.sourceFileName}}
                {{else}}
                    <a href="{{ rootPath }}{{module.id}}/index.html">{{module.id}}</a>
                {{/if}} :
                <span class="objects {{#if object.isDeprecated}}strike{{/if}}">{{ object.name }}</span>
            </h1>
            <p>
                {{#if object.isDeprecated}}
                    <span class="stab">Deprecated</span>
                {{/if}}
                {{{ object.description }}}
            </p>

            <div class="constants">
                <div class="method-sum">
                    {{#if object.initMethod}}
                        {{#object.initMethod}}
                        {{> initMethod }}
                        {{/object.initMethod}}
                    {{/if}}

                
                    {{#if object.otherMethods}}
                        <h2>Methods</h2>
                        <div class="method-list">
                            {{#each object.otherMethods}}
                                {{> methodSignature }}
                            {{/each}}
                        </div>
                    {{/if}}

                    {{#if object.fields}}
                        {{#object}}
                            <h2>Fields</h2>
                                {{> fields }}
                        {{/object}}
                    {{/if}}
                </div>

                {{#if object.otherMethods }}
                    {{#each object.otherMethods }}
                        {{> method }}
                    {{/each}}
                {{/if}}
            </div>
        </div>
    </div>

</body>

{{> footer }}
